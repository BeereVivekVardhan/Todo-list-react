{
  "name": "todolist",
  "version": "1.0.0",
  "private": true,
  "description": "Todo List Application with Express, React, TypeScript, and MongoDB",
  "scripts": {
    "start:frontend": "cd todo && npm run dev",
    "start:backend": "cd backend && npm run dev",
    "build:frontend": "cd todo && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm run start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
